<h2>Eventos</h2>
<hr>
<div class="">

  <% @eventos.each_with_index do |c, index| %>
    <div class="box panel panel-default masonry-brick">
          <a href="#"><img src="https://s3.amazonaws.com/fotoscomadronas/cards+para+website/Delicado.png" alt="Astronauta2"></a>
          <div class="panel-body">
            <strong><%= c.usuario %></strong>
            <br>
            <%= c.telefono %>
            <br>
            <%= c.fecha %>
            <br>
            <%= c.tipo %>
            <br>
          </div>
    </div>
  <% end %>
</div>
