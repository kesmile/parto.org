<h2>Lista de comandronas</h2>
<hr>
<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>#</th>
      <th>Nombre</th>
      <th>Direccion</th>
      <th>Codigo</th>
      <th>Acciones</th>
    </tr>
  </thead>
  <tbody>
    <% @comadronas.each_with_index do |c, index| %>
      <tr>
        <td><%= index %></td>
        <td><%= c.nombre %></td>
        <td><%= c.direccion %></td>
        <td><%= c.token %></td>
        <td><a href="#"><i class="fa fa-pencil"></i></a> <a href="#" ><i class="fa fa-trash-o"></i></a></td>
      </tr>
    <% end %>
  </tbody>
</table>
